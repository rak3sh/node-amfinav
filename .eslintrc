{
    "extends": "airbnb-base",
    "rules": {
        "linebreak-style": "off",
        "no-console": "off",
        "no-unused-vars": "warn",
        "no-use-before-define": "off",
        "no-spaced-func": "off",
        "eol-last": "off",
        "comma-dangle": "off",
        "max-len": "off",
        "func-names": "off",
        "mocha/no-exclusive-tests": "error",
        "no-shadow": ["error", {
            "allow": ["err"]
        }],
        "prefer-destructuring": "off"
    }
}